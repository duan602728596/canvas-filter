{
  "name": "@bbkkbkk/canvas-filter",
  "version": "0.0.0-alpha.1",
  "description": "canvas滤镜效果实现",
  "main": "dist/canvasFilter.js",
  "module": "lib/canvasFilter.js",
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development gulp",
    "build": "npm run clean && cross-env NODE_ENV=production gulp",
    "clean": "npm run clean:lib && npm run clean:dist",
    "clean:lib": "rimraf lib",
    "clean:dist": "rimraf dist",
    "example": "node ./scripts/www"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "browser-sync": "^2.26.7",
    "commitizen": "^3.1.2",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.8.0",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-terser": "^1.2.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "merge2": "^1.3.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.28.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint",
    "**/*.{ts,tsx}": "tslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
